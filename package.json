{
  "name": "redux-combine-reducers-immutable",
  "version": "2.0.15",
  "description": "Redux's combineReducers helper using Immutable Records",
  "contributors": [
    "Nicholas Gonzalez <nicky93@gmail.com>",
    "David Idol <david.idol@gmail.com>"
  ],
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "config": {
    "prettier_cmd": "prettier --single-quote --trailing-comma all --print-width 80 --list-different"
  },
  "scripts": {
    "compile": "gulp",
    "test": "NODE_ENV=test gulp test",
    "lint-ts": "tslint -c ./tslint.json --project ./tsconfig.json",
    "typecheck-ts": "tslint -c ./tslint.json --project ./tsconfig.json --type-check",
    "pretty": "$npm_package_config_prettier_cmd --write '{src, test}/**/*.{ts,tsx}' || exit 0",
    "pretty_dryrun": "$npm_package_config_prettier_cmd '{src, test}/**/*.{ts,tsx}'",
    "prepush": "echo 'Running typescript lint' && npm run lint-ts && echo 'Running typechecks' && npm run typecheck-ts && echo 'Verifying Prettier has been run' && (npm run pretty_dryrun || (echo 'Run `npm run pretty` before pushing' && exit 1))"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/di-ng/redux-combine-reducers-immutable.git"
  },
  "keywords": [
    "combineReducers",
    "redux",
    "immutable",
    "record"
  ],
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/di-ng/redux-combine-reducers-immutable/issues"
  },
  "homepage": "https://github.com/di-ng/redux-combine-reducers-immutable#readme",
  "peerDependencies": {
    "immutable": "3.x.x"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.2.0",
    "gulp-logger": "0.0.2",
    "gulp-mocha": "^5.0.0",
    "gulp-typescript": "^4.0.2",
    "husky": "^0.14.3",
    "immutable": "^3.8.2",
    "merge2": "^1.2.1",
    "mocha-better-spec-reporter": "^3.1.0",
    "prettier": "^1.11.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1"
  }
}
